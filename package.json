    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cross-env": "^7.0.0",
    "debug": "~2.6.9",
    "delay": "^5.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "html-entities": "^2.3.2",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "laravel-mix": "^6.0.39",
    "morgan": "~1.9.1",
    "p-retry": "^5.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
